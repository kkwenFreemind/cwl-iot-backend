{
  "properties": [
    {
      "name": "oss.type",
      "type": "java.lang.String",
      "description": "OSS type (currently supports aliyun, minio, local)"
    },
    {
      "name": "oss.minio.endpoint",
      "type": "java.lang.String",
      "description": "MinIO service endpoint"
    },
    {
      "name": "oss.minio.access-key",
      "type": "java.lang.String",
      "description": "MinIO access key"
    },
    {
      "name": "oss.minio.secret-key",
      "type": "java.lang.String",
      "description": "MinIO secret key"
    },
    {
      "name": "oss.minio.bucket-name",
      "type": "java.lang.String",
      "description": "MinIO bucket name"
    },
    {
      "name": "oss.minio.custom-domain",
      "type": "java.lang.String",
      "description": "MinIO custom domain"
    },
    {
      "name": "oss.local.storage-path",
      "type": "java.lang.String",
      "description": "Local file storage path"
    },
    {
      "name": "spring.cache.enabled",
      "type": "java.lang.Boolean",
      "description": "Cache enable/disable switch"
    },
    {
      "name": "security.session.type",
      "type": "java.lang.String",
      "description": "Session management type (jwt/redis-token)"
    },
    {
      "name": "security.session.access-token-time-to-live",
      "type": "java.lang.Integer",
      "description": "Access token time to live (seconds)"
    },
    {
      "name": "security.session.refresh-token-time-to-live",
      "type": "java.lang.Integer",
      "description": "Refresh token time to live (seconds)"
    },
    {
      "name": "security.session.jwt.secret-key",
      "type": "java.lang.String",
      "description": "JWT secret key (HS256 algorithm requires at least 32 characters)"
    },
    {
      "name": "security.session.redis-token.allow-multi-login",
      "type": "java.lang.Boolean",
      "description": "Allow multi-device login"
    },
    {
      "name": "captcha.type",
      "type": "java.lang.String",
      "description": "Captcha type (circle/gif/line/shear)"
    },
    {
      "name": "captcha.width",
      "type": "java.lang.Integer",
      "description": "Captcha width"
    },
    {
      "name": "captcha.height",
      "type": "java.lang.Integer",
      "description": "Captcha height"
    },
    {
      "name": "captcha.interfere-count",
      "type": "java.lang.Integer",
      "description": "Captcha interference element count"
    },
    {
      "name": "captcha.text-alpha",
      "type": "java.lang.Double",
      "description": "Captcha text transparency (0.0-1.0)"
    },
    {
      "name": "captcha.code.type",
      "type": "java.lang.String",
      "description": "Captcha code type (math/random)"
    },
    {
      "name": "captcha.code.length",
      "type": "java.lang.Integer",
      "description": "Captcha code length"
    },
    {
      "name": "captcha.font.name",
      "type": "java.lang.String",
      "description": "Captcha font name"
    },
    {
      "name": "captcha.font.weight",
      "type": "java.lang.Integer",
      "description": "Captcha font weight (0-normal/1-bold/2-italic)"
    },
    {
      "name": "captcha.font.size",
      "type": "java.lang.Integer",
      "description": "Captcha font size"
    },
    {
      "name": "captcha.expire-seconds",
      "type": "java.lang.Integer",
      "description": "Captcha expiration time (seconds)"
    }
